
<h1>
  Listado de lista de reproduccion
</h1>

<table>
  <thead>
    <tr>
      <th> Name </th>
      <th> Number of votes </th>
    </tr>
  </thead>

  <tbody>
    <% @playlists.each do |playlist| %>
      <tr>
        <th> <%= playlist.name %> </th>
        <th> <%= playlist.number_of_votes %> </th>
      </tr>
    <% end %>
  </tbody>


</table>
